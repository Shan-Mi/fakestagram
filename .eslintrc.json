{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "react/prop-types": 0,
    "no-console": 1
  },
  "plugins": ["react", "import", "jsx-a11y"],
  "parser": "babel-eslint",

  "settings": {
    "react": {
      "version": "detect" // Eslint, you figure our which version react we use. it will figure out for package.json.
    }
  }
}
